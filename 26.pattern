#include <stdio.h>
int main()
{
    int n;
    printf("B.MAHESH REDDY \n192210708\n");
	printf("Enter the no of rows:");
    scanf("%d",&n);
    for (int i = 1; i <= n; i++) {
        for (int j = 0; j < n - i; j++) {
            printf(" ");
        }
        for (int j = 1; j <= i; j++) {
            printf("%d ", j);
        }
        printf("\n");
    }
	return 0;
}
