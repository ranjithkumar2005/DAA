#include<stdio.h>
int fibo(int n)
{
	if(n==0)
	{
		return 0;
	}
	else if(n==1)
	{
		return 1;
	}
	else
	{
		return fibo(n-1)+fibo(n-2);
	}
}
int main()
{
	int l,i;
	printf("R.RANJITH KUMAR(192210687)\n");
	printf("enter the length:");
	scanf("%d",&l);
	printf("the elements are:\n");
	for(i=0;i<l;i++)
	{
		printf("%d ",fibo(i));
	}

}
