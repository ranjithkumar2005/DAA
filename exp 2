#include<stdio.h>
#include<math.h>
int main()
{
	int n,a,digit,c,s=0,sum=0;
	printf("R.RANJITHKUMAR 192210687 \n");
	printf("enter a int:");
	scanf("%d",&a);
	digit=a;
	c=a;
	while(c!=0)
	{
		c/=10;
		s++;
	}
	printf("no.of digits:%d\n",s);
	while(digit!=0)
	{
	    n=digit%10;
		sum+=pow(n,s);
		digit/=10;
	}
	if(sum==a)
	{
		printf("%d is an armstrong number",a);
	}
	else
	{
		printf("%d is not an armstrong number",a);
	}
}
